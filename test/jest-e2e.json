{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleNameMapper": {
    "^src/(.*)": "<rootDir>/../src/$1",
    "@app/vtex/(.*)": "<rootDir>/../libs/vtex/src/$1",
    "@app/vtex": "<rootDir>/../libs/vtex/src"
  },
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}